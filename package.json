{
  "name": "tool-box",
  "version": "0.0.3",
  "description": "A minimal electron application.",
  "main": "main.js",
  "scripts": {
    "server": "node server.js",
    "client": "electron .",
    "start": "electron .",
    "build": "electron-builder --win portable",
    "rebuild": "electron-rebuild -f -w main.js"
  },
  "repository": "https://github.com/omiinaya/system-tools",
  "author": "Omar Minaya",
  "license": "",
  "dependencies": {
    "iohook": "^0.9.3",
    "vigemclient": "^1.3.0"
  },
  "devDependencies": {
    "electron": "^11.3.0",
    "electron-builder": "22.4.0"
  },
  "iohook": {
    "arches": [
      "x64"
    ],
    "platforms": [
      "win32"
    ],
    "targets": [
      "electron-85"
    ]
  },
  "build": {
    "asar": true,
    "extraFiles": [
      "extraResources"
    ],
    "directories": {
      "buildResources": "resources"
    }
  }
}
